{
    "manifest_version": 3,
    "name": "Beatcode",
    "description": "LeetCode Extension",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": [
      "http://localhost:3000/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "brain2.png"
    }
  }